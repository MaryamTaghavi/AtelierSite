
@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en" dir="rtl">

<head>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="endless admin is super flexible, powerful, clean &amp; modern responsive bootstrap 4 admin template with unlimited possibilities.">
    <meta name="keywords" content="admin template, endless admin template, dashboard template, flat admin template, responsive admin template, web app">
    <meta name="author" content="pixelstrap">
    <link rel="icon" href="/AdminTemplate/assets/images/favicon.png" type="image/x-icon">
    <link rel="shortcut icon" href="/AdminTemplate/assets/images/favicon.png" type="image/x-icon">
    <title>Endless | Layout</title>
    <!-- Google font-->
    <link href="https://fonts.googleapis.com/css?family=Work+Sans:100,200,300,400,500,600,700,800,900" rel="stylesheet">
    <link href="https://fonts.googleapis.com/css?family=Nunito:200,200i,300,300i,400,400i,600,600i,700,700i,800,800i,900,900i" rel="stylesheet">
    <link href="https://fonts.googleapis.com/css?family=Poppins:100,100i,200,200i,300,300i,400,400i,500,500i,600,600i,700,700i,800,800i,900,900i" rel="stylesheet">
    <!-- Font Awesome-->
    <link rel="stylesheet" type="text/css" href="/AdminTemplate/assets/css/fontawesome.css">
    <!-- ico-font-->
    <link rel="stylesheet" type="text/css" href="/AdminTemplate/assets/css/icofont.css">
    <!-- Themify icon-->
    <link rel="stylesheet" type="text/css" href="/AdminTemplate/assets/css/themify.css">
    <!-- Flag icon-->
    <link rel="stylesheet" type="text/css" href="/AdminTemplate/assets/css/flag-icon.css">
    <!-- Feather icon-->
    <link rel="stylesheet" type="text/css" href="/AdminTemplate/assets/css/feather-icon.css">
    <!-- Plugins css start-->
    <!-- Plugins css Ends-->
    <!-- Bootstrap css-->
    <link rel="stylesheet" type="text/css" href="/AdminTemplate/assets/css/bootstrap.css">
    <!-- App css-->
    <link rel="stylesheet" type="text/css" href="/AdminTemplate/assets/css/style.css">
    <link id="color" rel="stylesheet" href="/AdminTemplate/assets/css/light-1.css" media="screen">
    <!-- Responsive css-->
    <link rel="stylesheet" type="text/css" href="/AdminTemplate/assets/css/responsive.css">
</head>
<body main-theme-layout="rtl">
    <!-- Loader starts-->
    <div class="loader-wrapper">
        <div class="loader bg-white">
            <div class="whirly-loader"> </div>
        </div>
    </div>
    <!-- Loader ends-->
    <!-- page-wrapper Start-->
    <div class="page-wrapper">
        <!-- Page Header Start-->
        <div class="page-main-header">
            <div class="main-header-right row">
                <div class="main-header-left d-lg-none">
                    <div class="logo-wrapper justify-content-center"><a href="#"><img style="width:100%; height:70px;" src="/AdminTemplate/assets/images/logo/logo.png" alt=""></a></div>
                </div>
                <div class="mobile-sidebar d-block">
                    <div class="media-body text-right switch-sm">
                        <label class="switch"><a href="#"><i id="sidebar-toggle" data-feather="align-left"></i></a></label>
                    </div>
                </div>
                <div class="nav-right col p-0">
                    <ul class="nav-menus">
                        <li>
                            <form class="form-inline search-form" action="#" method="get">
                                <div class="form-group">
                                    <div class="Typeahead Typeahead--twitterUsers">
                                        <div class="u-posRelative">
                                            <input class="Typeahead-input form-control-plaintext" id="demo-input" type="text" name="q" placeholder="جستجو...">
                                            <div class="spinner-border Typeahead-spinner" role="status"><span class="sr-only">در حال بارگذاری...</span></div><span class="d-sm-none mobile-search"><i data-feather="search"></i></span>
                                        </div>
                                        <div class="Typeahead-menu"></div>
                                    </div>
                                </div>
                            </form>
                        </li>

                       
                        <li class="onhover-dropdown">
                            <div class="media align-items-center">
                                <img class="align-self-center pull-right img-50 rounded-circle" src="/AdminTemplate/assets/images/user.png" alt="header-user">
                            </div>
                            <ul class="profile-dropdown onhover-show-div p-20">
                                <li><a href="#"><i data-feather="user"></i>ویرایش پروفایل</a></li>
                                <li><a href="#"><i data-feather="log-out"></i>خروج</a></li>
                            </ul>
                        </li>
                    </ul>
                    <div class="d-lg-none mobile-toggle pull-right"><i data-feather="more-horizontal"></i></div>
                </div>
                <script id="result-template" type="text/x-handlebars-template">
                    <div class="ProfileCard u-cf">
                    <div class="ProfileCard-avatar"><svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round" class="feather feather-airplay m-0"><path d="M5 17H4a2 2 0 0 1-2-2V5a2 2 0 0 1 2-2h16a2 2 0 0 1 2 2v10a2 2 0 0 1-2 2h-1"></path><polygon points="12 15 17 21 7 21 12 15"></polygon></svg></div>
                    <div class="ProfileCard-details">
                    <div class="ProfileCard-realName">{{name}}</div>
                    </div>
                    </div>
                </script>
                <script id="empty-template" type="text/x-handlebars-template">
                    <div class="EmptyMessage">جستجوی شما 0 نتیجه را به دست آورد. این احتمالا به این معنی است که باطن پایین است، وای!</div>

                </script>
            </div>
        </div>
        <!-- Page Header Ends                              -->
        <!-- Page Body Start-->
        <div class="page-body-wrapper">
            <!-- Page Sidebar Start-->
            <div class="page-sidebar">
                <div class="main-header-left d-none d-lg-block">
                    <div class="logo-wrapper  justify-content-center"><a href="index.html"><img style="width:100%; height:70px;" src="/AdminTemplate/assets/images/logo/logo.png" alt=""></a></div>
                </div>
                <div class="sidebar custom-scrollbar">
                    <ul class="sidebar-menu">
                        <li><a class="sidebar-header" href="./Dashboard.html"><i data-feather="home"></i><span>داشبورد</span></a></li>
                        <li><a class="sidebar-header" href="./Atelier/Ateliers.html"><i data-feather="home"></i><span>آتلیه ها</span></a></li>
                        <li><a class="sidebar-header" asp-controller="Grouping" asp-action="Index"><i data-feather="home"></i><span>گروه بندی</span></a></li>
                        <li><a class="sidebar-header" href="./City/Cities.html"><i data-feather="home"></i><span>شهر ها</span></a></li>
                        <li><a class="sidebar-header" href="./Photographer/Photographers.html"><i data-feather="home"></i><span>عکاس</span></a></li>
                    </ul>
                </div>
            </div>
            <div class="page-body">
                <div class="container-fluid">

                    @RenderBody() 

                </div>
            </div>
            <!-- footer start-->
            @*<footer class="footer">
                <div class="container-fluid">
                    <div class="row">
                        <div class="col-md-6 footer-copyright">
                            <p class="mb-0">کپی رایت 2018 © اندلس همه حقوق محفوظ است.</p>
                        </div>
                        <div class="col-md-6">
                            <p class="pull-right mb-0">دست ساز و ساخته شده با<i class="fa fa-heart"></i></p>
                        </div>
                    </div>
                </div>
            </footer>*@
        </div>
    </div>
    <!-- latest jquery-->
    <script src="/AdminTemplate/assets/js/jquery-3.2.1.min.js"></script>
    <!-- Bootstrap js-->
    <script src="/AdminTemplate/assets/js/bootstrap/popper.min.js"></script>
    <script src="/AdminTemplate/assets/js/bootstrap/bootstrap.js"></script>
    <!-- feather icon js-->
    <script src="/AdminTemplate/assets/js/icons/feather-icon/feather.min.js"></script>
    <script src="/AdminTemplate/assets/js/icons/feather-icon/feather-icon.js"></script>
    <!-- Sidebar jquery-->
    <script src="/AdminTemplate/assets/js/sidebar-menu.js"></script>
    <script src="/AdminTemplate/assets/js/config.js"></script>
    <!-- Plugins JS start-->
    <script src="/AdminTemplate/assets/js/chart/chartjs/chart.min.js"></script>
    <script src="/AdminTemplate/assets/js/chart/chartjs/chart.custom.js"></script>
    <script src="/AdminTemplate/assets/js/typeahead/handlebars.js"></script>
    <script src="/AdminTemplate/assets/js/typeahead/typeahead.bundle.js"></script>
    <script src="/AdminTemplate/assets/js/typeahead/typeahead.custom.js"></script>
    <script src="/AdminTemplate/assets/js/chat-menu.js"></script>
    <script src="/AdminTemplate/assets/js/tooltip-init.js"></script>
    <script src="/AdminTemplate/assets/js/typeahead-search/handlebars.js"></script>
    <script src="/AdminTemplate/assets/js/typeahead-search/typeahead-custom.js"></script>
    <!-- Plugins JS Ends-->
    <!-- Theme js-->
    <script src="/AdminTemplate/assets/js/script.js"></script>
    <script src="/AdminTemplate/assets/js/theme-customizer/customizer.js"></script>
    <!-- Plugin used-->
</body>

</html>